    "def calc(c, n1, n2):\n",
    "    if c == '+':\n",
    "        return n1 + n2\n",
    "    elif c == '-':\n",
    "        return n1 - n2\n",
    "    elif c == '*':\n",
    "        return n1 * n2\n",
    "    elif c == '/':\n",
    "        return n1 / n2"
   ]
